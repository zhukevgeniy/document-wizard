{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "env": {
    "development": {
      "presets": [
        ["@babel/preset-react", { "runtime": "automatic", "development": true }]
      ],
      "plugins": ["react-refresh/babel"]
    },
    "test": {
      "presets": [["@babel/preset-env", { "targets": { "node": "current" } }]]
    }
  }
}
